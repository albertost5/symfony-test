{% extends 'base.html.twig' %}

{% block title %}Sector
{% endblock %}

{% block body %}

	<div class="container">
		<div class="row mt-5">

			<div class="col">
				<h1>Sector</h1>
			</div>

		</div>

		<table class="table table-dark mt-4">
			<tbody>
				<tr>
					<th scope="col">Id</th>
					<td>{{ sector.id }}</td>
				</tr>
				<tr>
					<th scope="col">Nombre</th>
					<td>{{ sector.nombre }}</td>
				</tr>
			</tbody>
		</table>

		{{ include('sector/_delete_form.html.twig') }}
		<a class="btn btn-primary mt-1" href="{{ path('sector_index') }}">Cancelar</a>
	</div>
{% endblock %}
